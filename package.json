{
  "name": "top-notch-city",
  "main": "expo-router/entry",
  "version": "1.1.0",
  "scripts": {
    "start": "DARK_MODE=media APP_VARIANT=development expo start --dev-client --port 8082",
    "android": "DARK_MODE=media APP_VARIANT=development  expo run:android --port 8082",
    "ios": "DARK_MODE=media APP_VARIANT=development expo run:ios --port 8082",
    "prewidget": "npx expo prebuild --template node_modules/@bacons/apple-targets/prebuild-blank.tgz --clean",
    "web": "DARK_MODE=media expo start --web",
    "test": "jest --watchAll",
    "prebuild": "expo prebuild",
    "deploy": "npx expo export -p web && eas deploy --prod",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "gen": "graphql-codegen",
    "build": "NODE_ENV=production eas build",
    "drizzle:gen": "drizzle-kit generate:pg --schema ./src/drizzle/schema.ts --out=./src/drizzle/migrations",
    "drizzle:migrate": "bun run ./src/drizzle/migrate.ts",
    "drizzle:seed": "bun run ./src/drizzle/seed.ts"
  },
  "jest": {
    "preset": "jest-expo"
  },
