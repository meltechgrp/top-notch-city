{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"paths": {
			"@/*": ["./src/*"]
		}
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		"nativewind-env.d.ts",
		".expo/types/**/*.ts",
		"expo-env.d.ts",
		"metro.config.js",
		"src/components/3d-model.tsx",
		"src/components/price.jsx"
	],
	"exclude": ["node_modules"]
}
